{
    "name": "bkper-sheets",
    "version": "1.18.0",
    "description": "Bkper Sheets Add-on",
    "repository": "https://github.com/bkper/bkper-sheets",
    "author": "Bkper Inc.",
    "private": true,
    "scripts": {
        "clasp:store": "cp ~/.clasprc.json ~/.clasprc-bkper.json",
        "clasp:activate": "cp ~/.clasprc-bkper.json ~/.clasprc.json",
        "open": "clasp open",
        "test": "bun --cwd server/ test",
        "clean:dist": "rimraf dist",
        "clean:client": "bun --cwd client/ clean",
        "types": "bun --cwd client/ types",
        "clean:server": "bun --cwd server/ clean",
        "clean": "run-p clean:*",
        "upgrade:app": "bun --cwd server/ upgrade:app",
        "copy:client": "copyfiles -f client/dist/* dist/",
        "copy:server": "copyfiles -f server/src/* dist/",
        "copy": "run-p copy:*",
        "build:client": "bun --cwd client/ build",
        "build:server": "bun --cwd server/ build",
        "build": "run-p clean build:*",
        "postbuild": "bun copy",
        "dev": "run-p watch:client watch:server watch:push",
        "watch:client": "run-p watch:client:*",
        "watch:client:src": "watch 'bun --cwd client/ build' ./client/src",
        "watch:client:dist": "watch 'bun copy:client' ./client/dist",
        "watch:server": "watch 'bun copy:server' ./server/src",
        "watch:push": "watch 'bun push' ./dist --wait 5",
        "prepush": "bun clasp:activate",
        "push": "bun clasp push --force",
        "patch": "yarn version --patch",
        "minor": "yarn version --minor",
        "major": "yarn version --major",
        "preversion": "run-s build push",
        "postversion": "clasp version $npm_package_version && git push && git push --tags && echo \"Successfully released version $npm_package_version!\""
    },
    "devDependencies": {
        "@google/clasp": "^2.3.0",
        "copyfiles": "^2.1.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.0",
        "watch": "^1.0.2"
    },
    "dependencies": {}
}
